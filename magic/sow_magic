# A libmagic database containing definitions for files used by Shadow of War

# Shadow of War ARCH06 File
0x00    string      LTAR                Shadow of War ARCH06 File
>0x04   long        x                   Version %d
!:mime sow/arch06

# Shadow of War BNDL File
0x00    string      BNDL                Shadow of War BNDL File
>0x04   long        x                   Version %d
!:mime sow/bndl

# Shadow of War EMBB File
0x00    string      EMBB                Shadow of War EMBB File
>0x04   long        x                   Version %d
!:mime sow/embb

# Shadow of War FXP File
0x00    string      LTFX                Shadow of War FXP File
>0x04   long        x                   Version %d
!:mime sow/fxp

# Shadow of War MAT File
0x00    string      LTMI                Shadow of War MAT File
>0x04   long        x                   Version %d
!:mime sow/mat

# Shadow of War MESH File
